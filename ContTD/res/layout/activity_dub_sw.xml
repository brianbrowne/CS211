<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="#5089D6"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".DubSW" >

    <RelativeLayout
            android:id="@+id/RelLayDubSW"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >
    
    

    <TextView
        android:id="@+id/textview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="Contact Your TD!" />
    <TextView
        android:id="@+id/filler"
        android:layout_width="wrap_content"
        android:layout_height="20dip"
        android:layout_below="@+id/textview1" />
    
    
    
    <TextView
        android:id="@+id/textviewBriHay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/filler"
        android:textSize="20sp"
        android:text="Brian Hayes" />
    
    <EditText
        android:id="@+id/SubjectBriHay"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewBriHay"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45sp"
        android:layout_toRightOf="@+id/SubjectBriHay"
        android:src="@drawable/brianhayes" />
    
    <EditText
        android:id="@+id/messageBriHay"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectBriHay"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer162"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageBriHay" />
    
    <Button
        android:id="@+id/sendEmailBriHay"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer162"
        android:onClick="onClickSendEmail1"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer163"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailBriHay"/>
    
    <Button
        android:id="@+id/BriHaywebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer163"
        android:onClick="onClickSendEmailBriHay"
        android:text="Open Brian Hayes's website" />
    
    
    
    
    
    
    <TextView
        android:id="@+id/textviewSeaCro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="380dip"
        android:textSize="20sp"
        android:text="Sean Crowe" />
    
    <EditText
        android:id="@+id/SubjectSeaCro"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewSeaCro"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageViewSeaCro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="390dip"
        android:layout_toRightOf="@+id/SubjectSeaCro"
        android:src="@drawable/seancrowe" />
    
    <EditText
        android:id="@+id/messageSeaCro"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectSeaCro"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer164"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageSeaCro" />
    
    <Button
        android:id="@+id/sendEmailSeaCro"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer164"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer165"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailSeaCro" />
    
    <Button
        android:id="@+id/SeaCrowebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer165"
        android:text="Open Sean Crowe's website" />

</RelativeLayout>
</ScrollView>
