<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="#5089D6"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".DubSou" >

    <RelativeLayout
            android:id="@+id/RelLayDubSou"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >
    
    

    <TextView
        android:id="@+id/textview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="Contact Your TD!" />
    <TextView
        android:id="@+id/filler"
        android:layout_width="wrap_content"
        android:layout_height="20dip"
        android:layout_below="@+id/textview1" />
    
    
    
    <TextView
        android:id="@+id/textviewAleWhi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/filler"
        android:textSize="20sp"
        android:text="Alex White" />
    
    <EditText
        android:id="@+id/SubjectAleWhi"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewAleWhi"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45sp"
        android:layout_toRightOf="@+id/SubjectAleWhi"
        android:src="@drawable/alexwhite" />
    
    <EditText
        android:id="@+id/messageAleWhi"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectAleWhi"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer165"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageAleWhi" />
    
    <Button
        android:id="@+id/sendEmailAleWhi"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer165"
        android:onClick="onClickSendEmail1"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer166"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailAleWhi" />
    
    <Button
        android:id="@+id/AleWhiwebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer166"
        android:onClick="onClickSendEmailAleWhi"
        android:text="Open Alex White's website" />
    
    
    
    
    
    
    <TextView
        android:id="@+id/textviewOliMit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="380dip"
        android:textSize="20sp"
        android:text="Olivia Mitchell" />
    
    <EditText
        android:id="@+id/SubjectOliMit"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewOliMit"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageViewOliMit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="390dip"
        android:layout_toRightOf="@+id/SubjectOliMit"
        android:src="@drawable/oliviamitchel" />
    
    
    
    <EditText
        android:id="@+id/messageOliMit"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectOliMit"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer167"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageOliMit" />
    
    <Button
        android:id="@+id/sendEmailOliMit"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer167"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer168"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailOliMit" />
    
    <Button
        android:id="@+id/OliMitwebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer168"
        android:text="Open Olivia Mitchell's website" />
    
    
    
    
    
    
    <TextView
        android:id="@+id/textviewSheRos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="680dip"
        android:textSize="20sp"
        android:text="Shane Ross" />
    
    <EditText
        android:id="@+id/SubjectSheRos"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewSheRos"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageViewSheRos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="690dip"
        android:layout_toRightOf="@+id/SubjectSheRos"
        android:src="@drawable/shaneross" />
    
    <EditText
        android:id="@+id/messageSheRos"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectSheRos"
        android:hint="Please type your message"
        android:ems="10" />
    <TextView
        android:id="@+id/spacer169"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageSheRos" />
    
    <Button
        android:id="@+id/sendEmailSheRos"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer169"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer170"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailSheRos" />
    
    <Button
        android:id="@+id/SheRoswebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer170"
        android:text="Open Shane Ross's website" />

</RelativeLayout>
</ScrollView>