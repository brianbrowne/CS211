<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="#5089D6"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".DubNor" >

    <RelativeLayout
            android:id="@+id/RelLayDubNor"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >
    
    

    <TextView
        android:id="@+id/textview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="Contact Your TD!" />
    <TextView
        android:id="@+id/filler"
        android:layout_width="wrap_content"
        android:layout_height="20dip"
        android:layout_below="@+id/textview1" />
    
    
    
    <TextView
        android:id="@+id/textviewClaDal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/filler"
        android:textSize="20sp"
        android:text="Clare Daly" />
    
    <EditText
        android:id="@+id/SubjectClaDal"
    android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewClaDal"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45sp"
        android:layout_toRightOf="@+id/SubjectClaDal"
        android:src="@drawable/claredaly" />
    
    <EditText
        android:id="@+id/messageClaDal"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectClaDal"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer171"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageClaDal" />
    
    <Button
        android:id="@+id/sendEmailClaDal"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer171"
        android:onClick="onClickSendEmail1"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer172"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailClaDal" />
    
    <Button
        android:id="@+id/ClaDalwebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer172"
        android:onClick="onClickSendEmailClaDal"
        android:text="Open Clare Daly's website" />
    
    
    
    
    
    
    <TextView
        android:id="@+id/textviewAlaFar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="380dip"
        android:textSize="20sp"
        android:text="Alan Farrell" />
    
    <EditText
        android:id="@+id/SubjectAlaFar"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/textviewAlaFar"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageViewAlaFar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="390dip"
        android:layout_toRightOf="@+id/SubjectAlaFar"
        android:src="@drawable/alanfarrell" />
    
    <EditText
        android:id="@+id/messageAlaFar"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_height="55dip"
        android:layout_below="@+id/SubjectAlaFar"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer173"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageAlaFar" />
    
    <Button
        android:id="@+id/sendEmailAlaFar"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer173"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer174"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailAlaFar" />
    
    <Button
        android:id="@+id/AlaFarwebsite"
        android:layout_width="285dip"
        android:background="#32cd32"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer174"
        android:text="Open Alan Farrell's website" />
    
    
    
    </RelativeLayout>

</ScrollView>
