<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:background="#5089D6"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".RosSL" >

    <RelativeLayout
            android:id="@+id/RelLayMeW"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >
    
    

    <TextView
        android:id="@+id/textview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="Contact Your TD!" />
    <TextView
        android:id="@+id/filler"
        android:layout_width="wrap_content"
        android:layout_height="20dip"
        android:layout_below="@+id/textview1" />
    
    
    
    <TextView
        android:id="@+id/textviewFraFei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/filler"
        android:textSize="20sp"
        android:text="Frank Feighan" />
    
    <EditText
        android:id="@+id/SubjectFraFei"
        android:layout_height="55dip"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_below="@+id/textviewFraFei"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45sp"
        android:layout_toRightOf="@+id/SubjectFraFei"
        android:src="@drawable/frankfeighan" />
    
    <EditText
        android:id="@+id/messageFraFei"
        android:layout_height="55dip"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_below="@+id/SubjectFraFei"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer60"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageFraFei" />
    
    <Button
        android:id="@+id/sendEmailFraFei"
        android:layout_width="285dip"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer60"
        android:onClick="onClickSendEmail1"
        android:background="#32cd32"
        android:text="Send Email" />
    
    <TextView
        android:id="@+id/spacer61"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/sendEmailFraFei" />
    
         <Button
        android:id="@+id/FraFeiwebsite"
        android:layout_width="285dip"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer61"
        android:background="#32cd32"
        android:onClick="onClickSendEmailFraFei"
        android:text="Open Frank Feighan's website" />
    
    
    
    
    
    
    
    
    
    <TextView
        android:id="@+id/textviewLukFla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="380sp"
        android:textSize="20sp"
        android:text="Luke Ming Flanagan" />
    
    <EditText
        android:id="@+id/SubjectLukFla"
        android:layout_height="55dip"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_below="@+id/textviewLukFla"
        android:hint="Please type the subject of the message"
        android:ems="10" />
    
    <ImageView
        android:id="@+id/imageViewLukFla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="390sp"
        android:layout_toRightOf="@+id/SubjectLukFla"
        android:src="@drawable/lukemingflanagan" />
    
    <EditText
        android:id="@+id/messageLukFla"
        android:layout_height="55dip"
        android:layout_width="285dip"
        
        android:background="#32cd32"
        android:layout_below="@+id/SubjectLukFla"
        android:hint="Please type your message"
        android:ems="10" />
    
    <TextView
        android:id="@+id/spacer63"
        android:layout_width="200dip"   android:layout_height="4dip"
        android:layout_below="@+id/messageLukFla" />
    
    <Button
        android:id="@+id/sendEmailLukFla"
        android:layout_width="285dip"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spacer63"
        android:background="#32cd32"
        android:onClick="onClickSendEmail1"
        android:text="Send Email" />
    
       <!--  <Button
        android:id="@+id/LukFlawebsite"
        android:layout_width="200dip"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sendEmailLukFla"
        android:onClick="onClickSendEmailLukFla"
        android:text="Open Peader Toibin's website" />-->
       
      
</RelativeLayout>
</ScrollView>